<root>
<!--服务器内部消息在此定义-->
	<MSG_150000>
		<!--加载db_cache-->
	</MSG_150000>

	<MSG_550000>
    	<vector type="Player_DB_Cache" value="db_cache_vec"/>
	</MSG_550000>

	<MSG_150001>
		<!--加载玩家信息-->
    	<fixed type="Account_Info" value="account_info"/>
	</MSG_150001>

	<MSG_550001>
	</MSG_550001>

	<MSG_150002>
		<!--创建玩家-->
    	<fixed type="Game_Player_Info" value="player_info"/>
	</MSG_150002>

	<MSG_550002>
	</MSG_550002>

	<MSG_150003>
		<!--保存玩家信息-->
	</MSG_150003>

	<MSG_550003>
    	<arg type="int64" value="role_id"/>
	</MSG_550003>

	<MSG_150004>
		<!--保存邮件信息-->
    	<arg type="int64" value="role_id"/>
    	<fixed type="Mail_Detail" value="mail_detail"/>
	</MSG_150004>

	<MSG_150008>
		<!--删除数据库信息-->
    	<arg type="string" value="table_name" desc="删除数据表名称"/>
    	<vector type="int64" value="delete_list"/>
	</MSG_150008>

<!--MASTER到DB的消息-->

	<MSG_150101>
		<!--拉取数据库信息-->
    	<arg type="string" value="msg_type" desc="拉取数据表名称"/>
    	<arg type="int64" value="index" desc="索引值"/>
	</MSG_150101>

	<MSG_150102>
		<!--删除数据库信息-->
    	<arg type="string" value="table_name" desc="删除数据表名称"/>
    	<vector type="int64" value="delete_list"/>
	</MSG_150102>

	<MSG_150103>
		<!--保存公会信息-->
    	<arg type="int64" value="index"/>
    	<vector type="Guild_Info" value="guild_list"/>
	</MSG_150103>

	<MSG_550103>
    	<vector type="Guild_Info" value="guild_list"/>
	</MSG_550103>

	<MSG_150104>
		<!--保存离线信息-->
    	<arg type="int64" value="index"/>
    	<vector type="Offline_Info" value="offline_list"/>
	</MSG_150104>

	<MSG_550104>
    	<vector type="Offline_Info" value="offline_list"/>
	</MSG_550104>

	<MSG_150105>
		<!--保存排行榜信息-->
    	<arg type="int64" value="index"/>
    	<vector type="Rank_Info" value="rank_list"/>
	</MSG_150105>

	<MSG_550105>
    	<vector type="Rank_Info" value="rank_list"/>
	</MSG_550105>

<!--Log_Message--> 

	<MSG_180000>
		<!--后台MySQL数据库插入测试-->
    	<arg type="int32" value="id"/> 
    	<arg type="string" value="name"/>
	</MSG_180000>

	<MSG_180001>
		<!--后台登录退出流水-->
    	<arg type="int64" value="role_id"/>
    	<arg type="string" value="role_name"/>
    	<arg type="string" value="account"/>
    	<arg type="uint16" value="level"/>
    	<arg type="string" value="client_ip"/>
    	<arg type="int32" value="login_time"/>
    	<arg type="int32" value="logout_time"/>
    	<arg type="int32" value="online_time"/>
	</MSG_180001>
	
<!--Game_Master_Message-->

	<MSG_160000>
    	<fixed type="Master_Player_Info" value="player_info"/>
	</MSG_160000>

	<MSG_160001>
    	<arg type="int64" value="role_id"/>
	</MSG_160001>

	<MSG_160100> 		
		<!--master通知game设置公会信息-->
    	<arg type="int64" value="guild_id"/>
    	<arg type="string" value="guild_name"/>
	</MSG_160100>

	<MSG_165000>
    	<arg type="int32" value="level"/>
	</MSG_165000>
	
<!--Gate_Message-->

	<MSG_140000>
		<!--gate->login校验玩家-->
    	<arg type="string" value="account"/>
    	<arg type="string" value="session"/>
    	<arg type="string" value="gate_ip"/>
    	<arg type="int32" value="gate_port"/>
	</MSG_140000>

	<MSG_140001>
		<!--login->gate校验玩家结果-->
    	<arg type="string" value="account"/>
	</MSG_140001>

	<MSG_140100>
		<!--gate同步玩家下线到game-->
	</MSG_140100>

	<MSG_140200>
		<!--gate同步玩家上线到master-->
    	<fixed type="Master_Player_Info" value="player_info"/>
	</MSG_140200>

</root>
